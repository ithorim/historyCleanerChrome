{
    "manifest_version": 3,
    "name": "History Cleaner",
    "version": "1.0",
    "description": "Deletes history items containing specified keywords.",
    "permissions": [
      "history",
      "storage"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    }
  }